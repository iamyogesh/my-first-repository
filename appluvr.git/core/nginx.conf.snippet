    location /v2/master/api{
      proxy_pass  http://hanauma:8082/v2/api;
      proxy_redirect  http://hanauma/v2/api/ http://$host:$server_port/v2/master/api/;
      proxy_set_header X-REAL-IP $remote_addr;
      proxy_set_header X-Forwarded-For  $proxy_add_x_forwarded_for;
      proxy_set_header Host $host;
    }   
