    location /v2/master/views{
      proxy_pass  http://hanauma:8081/v2/views;
      proxy_redirect  http://hanauma/v2/views/ http://$host:$server_port/v2/master/views/;
      proxy_redirect  http://hanauma/v2/api/ http://$host:$server_port/v2/master/api/;
      proxy_set_header X-REAL-IP $remote_addr;
      proxy_set_header X-Forwarded-For  $proxy_add_x_forwarded_for;
      proxy_set_header Host $host;
    }  
